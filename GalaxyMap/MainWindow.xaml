<Window x:Name="window" x:Class="GalaxyMap.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GalaxyMap"
        xmlns:controls="clr-namespace:DeepZoom.Controls;assembly=DeepZoom"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Canvas x:Name="cavRoot">
            <Image x:Name="galaxyImage" Canvas.Left="0" Canvas.Top="0" Source="Images/galactic_full.jpg" Stretch="UniformToFill"/>
        </Canvas>
    </Grid>
</Window>
